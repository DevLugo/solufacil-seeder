{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Seeder - oldId 6620",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/.bin/ts-node",
            "args": ["src/standaloneApp.ts"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development"
            },
            "skipFiles": [
                "<node_internals>/**"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ]
        }
    ]
}

